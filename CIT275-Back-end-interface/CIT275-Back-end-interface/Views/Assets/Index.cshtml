@model IEnumerable<CIT275_Back_end_interface.Models.Asset>

@{
    ViewBag.Title = "Assets";
}

<h2>Assets</h2>

<p>
    @Html.ActionLink("Add New Asset", "Create")
    <br />
    @Html.ActionLink("Add New Asset Type", "Index", "AssetTypes")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AssetType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdentNbr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommissionedDt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DecommissionedDt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeploymentStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastMaintDt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeleteInd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssetType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdentNbr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommissionedDt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DecommissionedDt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeploymentStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastMaintDt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeleteInd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AssetID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AssetID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AssetID })
        </td>
    </tr>
}

</table>
